echo "# node" >> README.md
git init
git add README.md
git commit -m "first commit"
git remote add origin https://github.com/amarnath6232/node.git
git push -u origin master


// remove node_modules
git rm -r --cached node_modules

// remove last commit in local
git reset --hard HEAD~{parms}
Ex: git reset --hard HEAD~1

// remove last commit in remote
git push -f origin HEAD^:{branch}


// commit id to new branch starts
Go to commit id

git commit <commit id>

change the requirement and commit (new commit id generated)

git branch <new branch> <new commit id>

git add .

git commit -m "msg"

git checkout <new branch>

// commit id to new branch ends
